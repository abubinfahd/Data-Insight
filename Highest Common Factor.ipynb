     "text": [
      "15\n"
     ]
    }
   ],
   "source": [
    "def hcf(a, b):\n",
    "    if a > b:\n",
    "        smaller = b\n",
    "    else:\n",
    "        smaller = a\n",
    "\n",
    "    for i in range(1, smaller+1):\n",
    "        if a % i == 0 and b % i == 0:\n",
    "            ans = i\n",
    "    print(ans)\n",
    "\n",
    "hcf(30, 15)"
   ]
  }
